{
  "manifest_version": 2,
  "name": "Url Phishing Detector",
  "version": "1.0",

  "description": "A phishing detection extension.",

  "icons": {
    "48": "icons/border-48.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_icon": "icons/border-48.png",
    "default_title": "Security Settings",
    "default_popup": "popup.html"
  }
}