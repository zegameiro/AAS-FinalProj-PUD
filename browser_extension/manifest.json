{
  "manifest_version": 2,
  "name": "Borderify",
  "version": "1.0",

  "description": "A phishing detection extension.",

  "icons": {
    "48": "icons/border-48.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "tabs",
    "storage"
  ]
}